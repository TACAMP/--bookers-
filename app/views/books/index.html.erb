<h1>Books#index</h1>
<% @book.each do |book| %>

<p>Title</p>
<span><%= book.title %></span>
<p>Body</p>
<span><%= book.body %></span>

<%= link_to 'Show',"books/#{book.id}" %>
<%= link_to 'Edit',"books/#{book.id}/edit" %>
<%= link_to 'Destroy',book_path(book.id),"data-confirm"=>"Are you sure?" %>
<% end %>

